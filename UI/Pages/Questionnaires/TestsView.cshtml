@page
@using API.Controllers
@using APPCORE.Security
@{
    if (!SecurityController.Auth(HttpContext.Session.GetString("sessionKey")))
    {
        Response.Redirect("../LoginView");
        return;
    }   
    if (!SecurityController.HavePermission(Permissions.QUESTIONNAIRES_MANAGER.ToString(), HttpContext.Session.GetString("sessionKey")))
    {
        Response.Redirect("../Security/permission_error");
        return;
    }
}
<script src='~/Questionnaires/Views/TestsView.js' type='module'></script>
<div id='MainBody'></div>
